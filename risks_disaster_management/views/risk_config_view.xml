<?xml version="1.0" encoding="UTF-8" ?>
<odoo>


    <record id="risk_config_view" model="ir.ui.view">
        <field name="name">Risk config</field>
        <field name="model">risk.config</field>
        <field name="arch" type="xml">
            <tree>
                <field name="risk_name"/>
                <field name="risk_category"/>

                <field name="risk_description"/>

                <field name="risk_source"/>

                <field name="risk_cause"/>

                <field name="risk_start_datetime"/>

                <field name="risk_end_datetime"/>

                <field name="action_name"/>
                <field name="action_success"/>


            </tree>


        </field>
    </record>







        <record id="risk_view_config_response" model="ir.actions.act_window">
        <field name="name">risk config</field>
        <field name="res_model">risk.config</field>
        <field name="view_mode">tree</field>

    </record>






</odoo>